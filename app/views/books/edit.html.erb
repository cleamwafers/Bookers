<body>
<h1>Editing Book</h1>
<form>
<%= form_with model: @book, url: update_book_path(@book.id), method: :patch do |f| %>
   <label for="book_body">Title</label>
  <%= f.text_field :title %>

  <div class="field">
    <label for="book_body">Body</label>
  <%= f.text_area :body %>
  
</div>
  <%= f.submit 'Update Book' %>
</form>
  <a href="/books/1">show</a>
  |
  <a href="/books">Back</a>
</body>
<% end %>